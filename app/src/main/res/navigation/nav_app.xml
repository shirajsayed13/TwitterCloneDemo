<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_app"
    app:startDestination="@id/timeLineFragment">

    <fragment
        android:id="@+id/timeLineFragment"
        android:name="com.shiraj.twitterclonedemo.timeline.TimeLineFragment"
        android:label="fragment_time_line"
        tools:layout="@layout/fragment_time_line" >
        <action
            android:id="@+id/action_timeLineFragment_to_postTweetFragment"
            app:destination="@id/postTweetFragment" />
    </fragment>
    <fragment
        android:id="@+id/postTweetFragment"
        android:name="com.shiraj.twitterclonedemo.post.PostTweetFragment"
        android:label="fragment_post_tweet"
        tools:layout="@layout/fragment_post_tweet" />
</navigation>